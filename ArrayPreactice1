package JAVADSA;

import java.util.Scanner;

public class ArrayPractice1 {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);

        int n=sc.nextInt();
        float[] num=new float[n];
        for(int i=0;i<num.length;i++){
            num[i]=sc.nextFloat();
        }
        float sum=0;
        for(int i=0;i<num.length;i++){
            sum=sum+num[i];
        }
        System.out.println(sum);
    }
}
