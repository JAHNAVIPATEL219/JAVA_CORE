package JAVADSA.Methods;

public class Swap {
    public static void main(String[] args) {
        int a = 20;
        int b = 30;
        swapNum(a, b);
        System.out.println(a + "" + b);
        String name = "Kunal";
        changeName(name);
        System.out.println(name);
    }

    static void swapNum(int a, int b) {
        int temp = a;
        a = b;
        b = temp;
    }

    static void changeName(String name) {
        name = "janu";
    }
}
//Here values are not changing//
